package eu.stefaner.elasticlists.data {	import com.modestmaps.core.MapExtent;	/**	 * @author mo	 */	public class GeoConstraint extends FilterConstraint {		private var facet : GeoFacet;		private var filterExtent : MapExtent;		public function GeoConstraint(facet : GeoFacet, filterExtent : MapExtent) {			this.facet = facet;			this.filterExtent = filterExtent;		}		override public function match(c : ContentItem) : Boolean {			for (var facetValue:* in c.facetValues) {				if(facetValue is GeoFacetValue) {					if(filterExtent.contains((facetValue as GeoFacetValue).location)) {						return true;					}				}			}			return false;		}	}}